{
  "name": "react-boilerplate",
  "version": "0.0.0",
  "engines": {
    "node": "^10.0.0"
  },
  "scripts": {
    "build:dev": "webpack --mode development --config ./webpack.config.mjs -r @std/esm",
    "build:prd": "webpack --mode production --config ./webpack.config.mjs -r @std/esm",
    "start": "webpack-dev-server --config ./webpack.config.mjs -r @std/esm",
    "type-check": "tsc",
    "fmt": " eslint --fix --ext .tsx --ext .ts src/ && prettier --write src/**/*",
    "lint": " eslint --ext .tsx --ext .ts src/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,js}": "eslint",
    "*.{ts,tsx,js,jsx,mjs,html,scss,css,json,md}": [
      "npm run fmt",
      "git add"
    ]
  },
  "repository": "git@github.com:araya-playground/react-boilerplate.git",
  "author": "arayaryoma",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@types/react-router-dom": "^4.3.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-typescript": "^7.3.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@std/esm": "^0.26.0",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.2",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "autoprefixer": "^9.4.10",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "postcss": "^7.0.14",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "style-loader": "^0.23.1",
    "typescript": "^3.3.3333",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  }
}
