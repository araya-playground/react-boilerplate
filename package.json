{
  "name": "react-boilerplate",
  "version": "0.0.0",
  "engines": {
    "node": "^12.0.0"
  },
  "scripts": {
    "build:dev": "NODE_ENV=development webpack --mode development --config ./webpack.config.mjs -r @std/esm",
    "build:stg": "NODE_ENV=staging webpack --mode production --config ./webpack.config.mjs -r @std/esm",
    "build:prd": "NODE_ENV=production webpack --mode production --config ./webpack.config.mjs -r @std/esm",
    "start": "webpack-dev-server --config ./webpack.config.mjs -r @std/esm",
    "test": "yarn run type-check",
    "type-check": "tsc",
    "fmt": " eslint --fix --ext .tsx --ext .ts src/ && prettier --write src/**/*",
    "lint": " eslint --ext .tsx --ext .ts src/ --max-warnings 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,mjs}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{html,scss,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": "git@github.com:araya-playground/react-boilerplate.git",
  "author": "arayaryoma",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "normalize.css": "^8.0.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-typescript": "^7.3.2",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@std/esm": "^0.26.0",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.16",
    "@types/node": "^13.1.2",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.2",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "autoprefixer": "^9.4.10",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^5.0.1",
    "css-loader": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.0",
    "postcss": "^7.0.14",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-url": "^8.0.0",
    "prettier": "^1.16.4",
    "style-loader": "^1.1.2",
    "typescript": "^3.5.3",
    "url-loader": "^3.0.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  }
}
