{
  "compilerOptions": {
    "target": "esnext",
    "module": "es2015",
    "lib": ["dom", "esnext"],
    "jsx": "react",
    "noEmit": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "esModuleInterop": true
  },
  "include": ["src"]
}
